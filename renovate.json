{
    "extends": [
        "config:best-practices",
        "customManagers:dockerfileVersions",
        ":separatePatchReleases"
    ],
    "logLevelRemap": [
        {
            "matchMessage": "/.*Custom.*/",
            "newLogLevel": "info"
        }
    ],
  "customDatasources": {
    "dante": {
      "defaultRegistryUrlTemplate": "https://www.inet.no/dante/download.html",
      "format": "html",
      "transformTemplates": [
        "{ \"releases\": $.releases.{ \"version\": $match(version, /^\\/dante.*-(.*)\\.tar\\.gz$/).groups[0] }[version] }"
      ]
    }
  }
}
