{
    "extends": [
        "config:best-practices",
        "customManagers:dockerfileVersions",
        ":separatePatchReleases"
    ],
    "logLevelRemap": [
        {
            "matchMessage": "/.*Custom.*/",
            "newLogLevel": "info"
        }
    ],
  "customDatasources": {
    "prosody-modules": {
      "defaultRegistryUrlTemplate": "https://modules.prosody.im/rocks/",
      "format": "html",
      "transformTemplates": [
        "{ \"releases\": [ $.releases.{ \"version\": $match(version, /^{{packageName}}-(.+)-1\\.rockspec$/).groups[0] }[version] ] }"
      ]
    }
  }
}
